generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SY_USER {
  companyid  String   @db.VarChar
  id         String   @db.VarChar
  password   String   @db.VarChar
  employeeid String?  @db.VarChar
  language   String   @default("ko") @db.VarChar
  roles      String[]

  @@unique([companyid, id], map: "sy_user_companyid_idx")
  @@map("SY.USER")
}
